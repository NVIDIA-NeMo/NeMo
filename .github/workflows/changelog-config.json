{
    "categories": [
      {
        "title": "## ASR",
        "labels": ["asr", "rnnt", "transducer"]
      },
      {
        "title": "## TTS",
        "labels": ["tts"]
      },
      {
        "title": "## NLP / NMT",
        "labels": ["nlp", "nmt", "megatron"]
      }
    ],
    "ignore_labels": [
      "ignore"
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}\nUncategorized:\n${{UNCATEGORIZED}}\n\n",
    "pr_template": "- ${{TITLE}} :: PR: #${{NUMBER}}",
    "empty_template": "${{OWNER}}\n${{REPO}}\n${{FROM_TAG}}\n${{TO_TAG}}",
    "label_extractor": [
      {
        "pattern": "(.)+",
        "target": "$1",
        "flags": "gu"
      }
    ],
    "duplicate_filter": {
      "pattern": ".+",
      "on_property": "title",
      "method": "match"
    },
    "transformers": [
    ],
    "max_tags_to_fetch": 10,
    "max_pull_requests": 500,
    "max_back_track_time_days": 365,
    "exclude_merge_branches": [
    ],
    "tag_resolver": {
      "method": "semver"
    }
}